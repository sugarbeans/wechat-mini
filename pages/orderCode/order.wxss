.active{
  border-bottom: 4px solid #35c0ff;
}
page{
  background-color: #f6f9fb;
}
.order-bar{
  background-color: #ffffff;
  width: 100%;
  height: 50px;
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
}
.order-item{
  height: 50px;
  line-height: 50px;
  box-sizing: border-box;
}
.swiper-item{
  width: 100%;
  box-sizing: border-box;
  background-color: #f6f9fb;
  padding-top: 70px;
  overflow: hidden;
  overflow-y: auto;
}
.order-box{
  width: 90%;
  margin: 0 5% 20px 5%;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  padding: 0 10px;
  box-sizing: border-box;
  border-radius: 10px;
}
.order-title{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 40px;
  border-bottom: 1px solid #e5e5e5;
}
.order-status{
  font-size: 14px;
}
.order-main{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.order-main-l{
  display: flex;
  flex-direction: column;
  width: 80%;
}
.order-price{
  color: red;
}
.order-price>text:first-child{
  font-size: 14px;
}
.order-price>text:last-child{
  font-size: 20px;
}
.order-name{
  font-size: 16px;
  height: 40px;
  line-height: 40px;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.icon-q{
  font-size: 12px;
  margin-right: 6px;
  display: inline-block;
}
.order-code{
  height: 50px;
  line-height: 50px;
  z-index: 100;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 14px;
}
.order-code>view:last-child{
  margin-left: 20px;
  box-sizing: border-box;
}
.order-date,.order-num{
  font-size: 14px;
}
.order-qrcode{
  color: #35c0ff;
  font-size: 16px;
}
.icon-qrcode{
  font-size: 18px;
}
.order-td{
  display: inline-block;
  color: #35c0ff;
  font-size: 16px;
}
.qrcode-box{
  width: 100%;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.qrcode-btn{
  display: inline-block;
  font-size: 16px;
  height: 40px;
  line-height: 40px;
  padding: 0 10px;
  border-radius: 8px;
  background-color: #35c0ff;
  color: #ffffff;
}
.change-out-box{
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.change-title{
  width: 40%;
  height: 30px;
  line-height: 30px;
}
.change-box{
  width: 50%;
  height: 30px;
}
.change-box>text{
  display: inline-block;
  width: 33.33333%;
}
.change-box>text:nth-child(1){
  height: 30px;
  border: 1px solid #dcdcdc;
  box-sizing: border-box;
  text-align: center;
  line-height: 30px;
}
.change-box>text:nth-child(3){
  height: 30px;
  border: 1px solid #dcdcdc;
  box-sizing: border-box;
  text-align: center;
  line-height: 30px;
}
.change-box>text:nth-child(2){
  text-align: center;
  line-height: 30px;
}
.vister-out-box{
  width: 100%;
  text-align: left;
}
.checkbox{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.vister-box{
  width: 90%;
  display: flex;
  flex-direction: column;
}
.vister-item{
  font-size: 14px;
}
.islogin{
  width: 100%;
  height: 200px;
  line-height: 200px;
  text-align: center;
  color: #000000;
}
.swiper-box-item{
  width: 100px;
  height: 100px;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.swiper-box-item>image{
  width: 100%;
}
.swiper{
  width: 100%;
  height: 250px;
  text-align: center;
}
.weui-dialog__bd{
  text-align: center;
}
.qrcode-name{
  height: 20px;
  line-height: 20px;
  font-size: 16px;
}
