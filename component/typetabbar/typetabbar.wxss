.tab-bar-name{
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  height: 50px;
}
.tab-bar-item{
  width: 18%;
  margin: 0 1%;
  box-sizing: border-box;
  background-color: #e4ecf0;
  border-radius: 4px;
  text-align: center;
  font-size: 18px;
}
.active{
  background-color: #35c0ff;
  color: #ffffff;
}
