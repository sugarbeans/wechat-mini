@import '../../assets/style/iconfont.wxss';
.success-box{
  width: 100%;
  height: 300px;
  text-align: center;
  line-height: 300px;
}
.btn-box{
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.time-box{
  width: 100%;
  height: 70px;
  line-height: 70px;
  font-size: 18px;
  text-align: center;
}
.pay-success{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.pay-success>icon{
  height: 70px;
}
.pay-success>text{
  font-size: 26px;
  display: block;
  height: 190px;
  line-height: 190px;
}
